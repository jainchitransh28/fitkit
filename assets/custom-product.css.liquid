.main_prod_section {
  padding: 40px 15px 0px;
  margin-bottom: 100px;
}
.product__info-wrapper {
/* padding: 55px 0px 0px 80px;  */
  padding: 55px 0px 0px 40px;
}
media-gallery .product__media-icon {
  left: 20px;
  top: 20px;
  display: none;
}
media-gallery .product__media, media-gallery .product__media * { 
  border-radius: 40px;
}
.product__media-gallery .product__media-list .product__media-item {
  width: 100%;
  max-width: 100%;
}
.product .price--sold-out .price__badge-sold-out {
  background-color: #ea6463;
  border-color: #ea6463;
  color: #fff;
}
.product__info-wrapper .custom_prod_metafied {
  font-size: calc(var(--font-body-scale) * 17px);
  line-height: calc(var(--font-body-scale) * 22px);
}
.product__info-wrapper .price  {
  font-size: calc(var(--font-body-scale) * 17px);
  line-height: calc(var(--font-body-scale) * 22px);
  font-weight: 700;
}
.product-form__input {
  max-width: 100%;
}
.product-form__input .form__label {
  font-size: calc(var(--font-body-scale) * 15px);
  letter-spacing: 0px;
  line-height: calc(var(--font-body-scale) * 18px);
}
.select-pack-size--name .form__label:after {
  content: "(Discounts included)";
  color: #707070;
  font-size: 12px;
  line-height: 14px;
  vertical-align: middle;
}
.product-form__input--dropdown .select {
 max-width: 380px;
}
.product-form__input--dropdown .select select {
  min-height: auto;
  height: auto;
  padding: 12px 20px;
  border: 1px solid #F5F2F2;
  background-color: #F5F2F2;
  font-size: 18px;
  line-height: 22px;
  color: rgba(var(--color-foreground),1);
  border-radius: 10px;
}
.product-form__input--dropdown .select:after, .product-form__input--dropdown .select:before {
  display: none;
}
.product-form__input--dropdown .select__select:focus {
  box-shadow: none;
}
.product__info-wrapper .product-form__buttons {
  max-width: 100%;
  display: flex;
  flex-wrap: wrap;
}
.product__info-wrapper .shopify-payment-button, .product__info-wrapper  .product-form__buttons .product-form__submit, 
.product__info-wrapper  .product-form__buttons .button {
  width: calc(50% - 15px);
  max-width: 350px;
  margin-right: 15px;
}
.product__info-wrapper  .product-form__buttons .button {
  margin-bottom: 1rem;   
}
.product__info-wrapper  .product-form__buttons .product-form__submit {
  background-color: #473247;
  color: #fff;
  border: 2px solid #473247;
}
.product__info-wrapper .product-form__buttons .product-form__submit, .product__info-wrapper .product-form__buttons .shopify-payment-button__button--unbranded {
  padding: 11px 20px;
}
.product__info-wrapper .product-form__buttons .product-form__submit:hover {
  background-color: transparent;
  color: #473247;
}
.product__info-wrapper .product-form__buttons .shopify-payment-button__button--unbranded  {
  text-transform: uppercase;
  min-height: auto;
  height: auto;
  font-size: calc(var(--font-body-scale) * 13px);
  line-height: calc(var(--font-body-scale) * 16px);
  border: 2px solid rgba(var(--color-button),var(--alpha-button-background));
}
.product__info-wrapper .product-form__buttons .shopify-payment-button__button--unbranded:hover {
  background-color: transparent;
    color: rgba(var(--color-button),var(--alpha-button-background));
}
.product__info-wrapper .product_rating_block .jdgm-widget.jdgm-preview-badge {
  margin: 20px 0px;
}
.product__info-wrapper .product_rating_block .jdgm-prev-badge__text {
  font-size: calc(var(--font-body-scale) * 13px);
  line-height: calc(var(--font-body-scale) * 16px);
  padding-left: 5px;
}
.product__info-wrapper .jdgm-preview-badge .jdgm-star.jdgm-star {
  font-size: 14px;
  margin-right: 4px !important;
}
.product__info-container .product__description {
  margin: 0px 0px 30px;
}
.product__info-wrapper .product__title {
  margin-bottom: 1.5rem;
}
.product__info-wrapper .product__title h1 {
  margin-bottom: 5px;
}
.btn_bundle_area .button.button--primary  {
  background-color: #473247;
  border: 2px solid #473247;
  color: #fff;
}
.btn_bundle_area .button.button--primary:hover  {
  background-color: transparent;
  color: #473247;
}

media-gallery .thumbnail-slider button.slider-button {
  width: 19px;
  height: auto;
  border-radius: 10px;
  border: 1px solid rgba(112,112,112,0.30);
  position: relative;
  align-self: stretch;
  margin-top: 0.5rem;
  margin-bottom: 0.5rem;
}
media-gallery .thumbnail-slider button.slider-button svg {
  display: none;
}
media-gallery .thumbnail-slider button.slider-button[disabled="disabled"] {
  opacity: 0.4;
}
media-gallery .thumbnail-slider .slider-button--prev {
  margin-right: 10px;
}
media-gallery .thumbnail-slider .slider-button--next {
  margin-left: 10px;
}
media-gallery .thumbnail-slider .slider-button--prev:after, media-gallery .thumbnail-slider .slider-button--next:after {
  content: "";
  position: absolute;
  top: 50%;
  -o-transform: translateY(-50%);
  -moz-transform: translateY(-50%);
  -ms-transform: translateY(-50%);
  -webkit-transform: translateY(-50%);
  transform: translateY(-50%);
}
media-gallery .thumbnail-slider .slider-button--prev:after {
  border-top: 4px solid transparent;
  border-bottom: 4px solid transparent;
  border-right: 8px solid rgba(112,112,112,0.30);
}
media-gallery .thumbnail-slider .slider-button--next:after {
  border-top: 4px solid transparent;
  border-bottom: 4px solid transparent;
  border-left: 8px solid rgba(112,112,112,0.30);
}
media-gallery .thumbnail-list .thumbnail {
  border-radius: 17px;
}
media-gallery .thumbnail-slider .thumbnail[aria-current] {
  box-shadow: none;
  border-color: transparent;
}
.custom_bundle_prod .prod_bundle_title {
  font-size: calc(var(--font-body-scale) * 16px);
  line-height: calc(var(--font-body-scale) * 21px);
  font-weight: 700;
  margin-bottom: 15px;
}
.custom_bundle_prod .bundle_row {
  border-radius: 30px;
  border: 1px solid rgb(112 112 112 / 65%);
  display: flex;
  flex-wrap: wrap;
}
.custom_bundle_prod .bundle_left_box {
  flex: 0 0 28%;
  max-width: 28%;
}
.custom_bundle_prod .bundle_left_box .bundle_img_entry {
  border-radius: 30px;
/*   background: #f6f6f6; */
  overflow: hidden;
/*   height: 100%; */
}
.custom_bundle_prod .bundle_img_entry img {
  display: block;
/*   height: 100%; */
/*   object-fit: cover; */
}
.custom_bundle_prod .bundle_right_box {
  flex: 0 0 72%;
  max-width: 72%;
  padding: 2.5% 4%;
}
.custom_bundle_prod .bundle_title {
  font-size: calc(var(--font-body-scale) * 16px);
  line-height: calc(var(--font-body-scale) * 20px);
  text-transform: uppercase;
  margin-bottom: 5px;
}
.custom_bundle_prod .bundle_des {
  margin-bottom: 10px;
}
.btn_bundle_area .button {
  min-width: 100%;
}
.product .jdgm-rev-widg__title {
  display: none;
}
product-modal .product-media-modal__content {
  display: flex;
  flex-wrap: wrap;
  justify-content: center;
}
product-modal .product-media-modal__content .global-media-settings {
  flex: 0 0 calc(60% - 20px);
  max-width: calc(60% - 20px);
  width: calc(60% - 20px);
  margin: 10px;
}
.product__info-wrapper .price > * {
  display: inline-block;
  vertical-align: middle;
}
.product__description.rte ul {
   list-style: disc; 
}
.custom_bundle_prod .wrapper_form .btn_id_wrapper {
  display: flex;
}
.custom_bundle_prod .wrapper_form .id_wrapper {
  flex: 0 0 auto;
  max-width: 100%;
}
.custom_bundle_prod .id_wrapper select {
  width: 120px;
  padding: 12px 15px;
  border: 1px solid #F5F2F2;
  background-color: #f5f2f2;
  font-size: 16px;
  line-height: 20px;
  color: rgba(var(--color-foreground),1);
  border-radius: 10px;
  cursor: pointer;
  margin-right: 8px;
}
.custom_bundle_prod .wrapper_form .btn_wrapper {
  flex: 1 0 0;
  max-width: 100%;
}
.custom_bundle_prod .wrapper_form .button {
    height: 100%;
    background-color: #473247;
    color: #fff;
    border: 2px solid #473247;
    border-radius: 10px;
    width: 100%;
    min-width: auto;
}
.custom_bundle_prod .wrapper_form .button:hover {
  background-color: transparent;
  color: #473247;
}
.custom_bundle_prod .button .compare_price {
   margin-left: 5px;
  color: #EA6463;
  text-decoration: line-through;
}
.custom_bundle_prod .button .main_price {
  margin-left: 5px;
}
.custom_bundle_prod .product-form {
  margin: 0px;
}
.product__info-wrapper .price__regular {
  margin-right: 1rem;
}



@media screen and (min-width: 750px) {
  .thumbnail-list.slider--tablet-up .thumbnail-list__item.slider__slide {
    width: calc(20% - 0.8rem);
  }  
}

@media screen and (min-width: 900px) {
  .product--small .thumbnail-list.slider--tablet-up .thumbnail-list__item.slider__slide {
    width: calc(20% - 0.8rem);
  }
}

@media screen and (max-width: 1680.98px) {
   .main_prod_section {
    padding: 20px 15px 0;
    margin-bottom: 60px;
  }
  .product__info-wrapper {
  /* padding: 35px 0 0 60px; */
    padding: 35px 0 0 20px;  
  }
  .product__info-wrapper .custom_prod_metafied {
    font-size: calc(var(--font-body-scale) * 15px);
    line-height: calc(var(--font-body-scale) * 20px);
  }
}

@media screen and (max-width: 1367.98px) {
  .custom_bundle_prod .bundle_title {
    font-size: calc(var(--font-body-scale) * 13px);
    line-height: calc(var(--font-body-scale) * 16px);
    margin-bottom: 0;
  }
  .custom_bundle_prod .id_wrapper select {
    font-size: 13px;
    line-height: 17px;
  }
  .custom_bundle_prod .bundle_right_box {
    padding: 2% 4%;
  }
  .custom_bundle_prod .bundle_des {
    margin-bottom: 5px;
  }
}

@media screen and (max-width: 1280.98px) {
/*   .product__info-wrapper {
    padding: 35px 0 0 40px;
  } */
  .product__info-wrapper .custom_prod_metafied {
    font-size: calc(var(--font-body-scale) * 13px);
    line-height: calc(var(--font-body-scale) * 18px);
  }
  .product-form__input .form__label {
    font-size: calc(var(--font-body-scale) * 13px);
    line-height: calc(var(--font-body-scale) * 16px);
  }
  
}

@media screen and (max-width: 1199.98px) {
  product-modal .product-media-modal__content .global-media-settings {
    flex: 0 0 calc(100% - 20px);
    max-width: calc(100% - 20px);
    width: calc(100% - 20px); 
  }  
}

@media screen and (max-width: 989.98px) {
  .product__info-wrapper.grid__item {
    padding: 15px 0 0 0px;
    max-width: 500px; 
    margin: 0 auto;
  } 
  .product__media-wrapper.grid__item {
    max-width: 500px;
    margin: 0 auto; 
  }
  .product__info-container {
    max-width: 100%;
  }
  .custom_bundle_prod {
    margin-top: 50px;
    margin-bottom: 0px;
  } 
  .main_prod_section {
    padding: 5px 15px 0px;
  }
  .product__info-container .product-form {
    margin-bottom: 0px;
  }
  
}

@media screen and (max-width: 767.98px) {
  .product__info-wrapper .product-form__buttons .shopify-payment-button__button--unbranded {
    font-size: calc(var(--font-body-scale) * 10px);
    line-height: calc(var(--font-body-scale) * 13px);
  }
  .product__info-wrapper .product-form__buttons .product-form__submit, .product__info-wrapper .product-form__buttons .shopify-payment-button__button--unbranded {
    padding: 9px 20px;
  }
  .product-form__input--dropdown .select select {
    padding: 7px 20px;
    font-size: 14px;
    line-height: 18px;
  }
  .product-form__input .form__label {
    font-size: calc(var(--font-body-scale) * 12px);
    line-height: calc(var(--font-body-scale) * 14px);
  }
  .custom_bundle_prod .prod_bundle_title {
    font-size: calc(var(--font-body-scale) * 14px);
    line-height: calc(var(--font-body-scale) * 18px);
    text-align: center;
  }
  .custom_bundle_prod .bundle_title {
    font-size: calc(var(--font-body-scale) * 11px);
    line-height: calc(var(--font-body-scale) * 16px);
  }
  .custom_bundle_prod .bundle_row {
    border-radius: 15px;
  }
  .custom_bundle_prod .bundle_left_box .bundle_img_entry {
    border-radius: 15px;
  }
  media-gallery .thumbnail-list .thumbnail {
    border-radius: 10px;
  }
  .product__info-wrapper .price {
    font-size: calc(var(--font-body-scale) * 12px);
    line-height: calc(var(--font-body-scale) * 15px);
  }
  .price .badge {
    font-size: 1rem;
    padding: 0.3rem 1rem 0.2rem;
  }
  .product__info-wrapper .jdgm-preview-badge .jdgm-star.jdgm-star {
    font-size: 10px;
    margin-right: 1px!important;
  }
  .product__info-wrapper .jdgm-prev-badge__text {
    font-size: 12px;
  }
  .custom_bundle_prod .bundle_left_box {
    flex: 0 0 32%;
    max-width: 32%;
  }
  .custom_bundle_prod .bundle_right_box {
    flex: 0 0 68%;
    max-width: 68%;
  }
  .custom_bundle_prod .bundle_des {
    margin-bottom: 10px;
    font-size: calc(var(--font-body-scale) * 10px);
    line-height: calc(var(--font-body-scale) * 18px);
  }
  .custom_bundle_prod .wrapper_form .id_wrapper {
    display: none;
  }
}

@media screen and (max-width: 749px) {
  .thumbnail-list__item.slider__slide {
    width: calc(20% - 0.8rem);
  }
  .grid--peek.slider .grid__item:first-of-type {
    margin-left: 0;
  }
  .product__media-wrapper slider-component:not(.thumbnail-slider--no-slide) {
    margin-left: 0rem;
    margin-right: 0rem;
  }
  media-gallery .thumbnail-slider button.slider-button {
    display: none;
  }
  ul.slider.slider--mobile {
    scroll-padding-left: 0;
  }
  .product__info-container .product__description {
    margin: 15px 0 30px;
  }
  .product__info-wrapper .price__regular {
    margin-right: 0.5rem;
  }
}


@media screen and (max-width: 575.98px) {
  .product__info-wrapper .product-form__buttons .product-form__submit, .product__info-wrapper .shopify-payment-button,
  .product__info-wrapper .product-form__buttons .button {
    margin-right: 0;
    width: 100%;
    max-width: 100%;
  }
  .product-form__input--dropdown .select {
    max-width: 100%;
  }
  .product__info-wrapper variant-selects {
     margin-bottom: 15px; 
  }
  
}






